import NextAuth from 'next-auth';
import { authOptions } from 'next-auth'; // Adjust provider as needed

export const authOptions = {
  providers: [
    /
  ],
  callbacks: {
    async session({ session, user }) {
      session.user.id = user.id; // Ensure Clerk ID is included
      return session;
    },
  },
};

export default NextAuth(authOptions);
